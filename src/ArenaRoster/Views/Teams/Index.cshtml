@{
    ViewData["Title"] = "Teams";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model List<ArenaRoster.Models.Team>

<h1>List of Teams</h1>
<hr />
@foreach(Bar.Models.Team team in Model)
{
    <div class="card">
        <div class="card-content">
            <h4><a href="/Teams/Details/@team.TeamId">@team.Name</a></h4>
        </div>
    </div>
}